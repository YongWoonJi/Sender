<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="16dp"
    tools:context="com.sender.team.sender.InfoInputFragment">

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/edit_object_name"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="16dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView22"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/ic_time" />

        <TextView
            android:id="@+id/edit_infoinput_hour"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="2dp"
            android:layout_weight="1"
            android:background="@null"
            android:hint="도착 요청시간"
            android:paddingBottom="2dp"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:text="시"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/edit_infoinput_min"
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:layout_marginRight="2dp"
            android:background="@null"
            android:gravity="right"
            android:textSize="12sp" />


        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="분"
            android:textSize="12sp" />


    </LinearLayout>

    <Button
        android:id="@+id/btn_deliverer"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:background="@color/fontcolor"
        android:text="@string/deliverer_search"
        android:textColor="@color/whitefont"
        android:textSize="12sp" />


    <EditText
        android:id="@+id/edit_object_name"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="16dp"
        android:layout_toEndOf="@+id/imageView21"
        android:layout_toRightOf="@+id/imageView21"
        android:background="@null"
        android:hint="@string/object_name"
        android:inputType="text"
        android:paddingBottom="2dp"
        android:singleLine="true"
        android:textSize="12sp" />

    <EditText
        android:id="@+id/edit_receiver_phone"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/imageView28"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="16dp"
        android:layout_toEndOf="@+id/imageView24"
        android:layout_toRightOf="@+id/imageView24"
        android:background="@null"
        android:hint="@string/receiver_phone"
        android:inputType="phone"
        android:paddingBottom="2dp"
        android:textSize="12sp" />

    <EditText
        android:id="@+id/edit_object_price"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/imageView23"
        android:layout_marginLeft="10dp"
        android:layout_toEndOf="@+id/imageView23"
        android:layout_toRightOf="@+id/imageView23"
        android:background="@null"
        android:hint="@string/price_suggestion"
        android:inputType="number"
        android:paddingBottom="2dp"
        android:textSize="12sp" />

    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_alignLeft="@+id/imageView26"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/imageView26"
        android:src="@drawable/ic_name" />


    <ImageView
        android:id="@+id/imageView23"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_alignLeft="@+id/linearLayout5"
        android:layout_alignStart="@+id/linearLayout5"
        android:layout_below="@+id/linearLayout5"
        android:layout_marginTop="16dp"
        android:src="@drawable/ic_money" />

    <ImageView
        android:id="@+id/imageView24"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_alignLeft="@+id/linearLayout4"
        android:layout_alignStart="@+id/linearLayout4"
        android:layout_alignTop="@+id/edit_receiver_phone"
        android:src="@drawable/ic_phonebook" />

    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/imageView24"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp">

        <ImageView
            android:id="@+id/imageView25"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_alignLeft="@+id/imageView24"
            android:layout_alignStart="@+id/imageView24"
            android:layout_below="@+id/imageView24"
            android:layout_marginTop="16dp"
            android:src="@drawable/ic_memo" />

        <EditText
            android:id="@+id/edit_memo"
            android:layout_width="wrap_content"
            android:layout_height="65dp"
            android:layout_alignBottom="@+id/imageView25"
            android:layout_alignEnd="@+id/object_image"
            android:layout_alignRight="@+id/object_image"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="16dp"
            android:layout_toRightOf="@+id/imageView25"
            android:layout_weight="1"
            android:background="@null"
            android:gravity="top"
            android:hint="메모"
            android:paddingBottom="2dp"
            android:textSize="12sp" />

        <ImageView
            android:id="@+id/object_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/imageView31"
            android:layout_alignRight="@+id/imageView31"
            android:layout_below="@+id/edit_memo"
            android:background="#e6e6e6"
            android:src="@drawable/ic_photo"
            android:layout_marginTop="16dp" />
    </LinearLayout>

    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_alignLeft="@+id/btn_deliverer"
        android:layout_alignStart="@+id/btn_deliverer"
        android:layout_below="@+id/edit_object_name"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:background="@color/grayline" />

    <ImageView
        android:id="@+id/imageView27"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_alignLeft="@+id/linearLayout5"
        android:layout_alignStart="@+id/linearLayout5"
        android:layout_below="@+id/linearLayout5"
        android:layout_marginRight="16dp"
        android:background="@color/grayline" />

    <ImageView
        android:id="@+id/imageView28"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_alignLeft="@+id/imageView27"
        android:layout_alignStart="@+id/imageView27"
        android:layout_below="@+id/edit_object_price"
        android:layout_marginRight="16dp"
        android:background="@color/grayline" />

    <ImageView
        android:id="@+id/imageView29"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_alignLeft="@+id/imageView28"
        android:layout_alignStart="@+id/imageView28"
        android:layout_below="@+id/edit_receiver_phone"
        android:layout_marginRight="16dp"
        android:background="@color/grayline" />

    <ImageView
        android:id="@+id/imageView31"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/linearLayout4"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:background="@color/grayline" />

    <Button
        android:layout_width="50dp"
        android:layout_height="20dp"
        style="?android:attr/borderlessButtonStyle"
        android:text="주소록"
        android:id="@+id/btn_contacts"
        android:textSize="11sp"
        android:textColor="@color/whitefont"
        android:gravity="center"
        android:background="@color/fontcolor"
        android:layout_above="@+id/linearLayout4"
        android:layout_alignRight="@+id/linearLayout4"
        android:layout_alignEnd="@+id/linearLayout4"
        android:elevation="0dp"
        android:translationZ="0dp"
        android:layout_marginBottom="3dp"/>


</RelativeLayout>
